{
  "name": "@vibe-kit/cloudflare",
  "version": "0.0.6",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "Dockerfile",
    "patches/"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@vibe-kit/sdk": "*"
  },
  "dependencies": {
    "@cloudflare/sandbox": "^0.1.3",
    "patch-package": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "wrangler": "^4.26.0"
  }
}